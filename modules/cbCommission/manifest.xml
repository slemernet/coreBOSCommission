<tables>
<table>
<name>vtiger_cbcommission</name>
<sql><![CDATA[CREATE TABLE `vtiger_cbcommission` (
  `cbcommissionid` int(11) default NULL,
  `cbcommissionno` varchar(56) default NULL,
  `percentage` decimal(7,3) DEFAULT '0.000',
  `amount` decimal(25,4) DEFAULT NULL,
  `soinvtotal` decimal(25,4) DEFAULT NULL,
  `commission` decimal(25,4) DEFAULT NULL,
  `employeeid` int(11) default NULL,
  `soinvid` int(11) default NULL,
  PRIMARY KEY (`cbcommissionid`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8]]></sql>
</table>
<table>
<name>vtiger_cbcommissioncf</name>
<sql><![CDATA[CREATE TABLE `vtiger_cbcommissioncf` (
  `cbcommissionid` int(11) NOT NULL,
  PRIMARY KEY (`cbcommissionid`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8]]></sql>
</table>
